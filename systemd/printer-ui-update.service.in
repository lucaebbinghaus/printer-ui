[Unit]
Description=Printer UI Update
After=network-online.target docker.service
Wants=network-online.target
Requires=docker.service

[Service]
Type=oneshot
User=@APP_USER@
WorkingDirectory=/opt/printer-ui
ExecStart=/opt/printer-ui/scripts/update.sh
TimeoutStartSec=0

[Install]
WantedBy=multi-user.target
